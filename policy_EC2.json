{
  "Version": "2012-10-17",
  "Statement": [{
    "Effect": "Allow",
    "Action": [
      "elasticloadbalancing:Describe*",
      "ec2:DescribeInstances",
      "rds:*",
      "dynamodb:DescribeTable",
      "dynamodb:GetItem",
      "dynamodb:PutItem",
      "s3:*",
      "sqs:SendMessage"
    ],
    "Resource": ["*"]
  },
  {
    "Effect": "Allow",
    "Action": ["sns:ListTopics",
      "sns:Publish",
      "sqs:DeleteMessage",
      "sqs:GetQueueAttributes",
      "sqs:PurgeQueue",
      "sqs:ReceiveMessage",
      "sqs:SetQueueAttributes",
      "sqs:GetQueueUrl",
      "sqs:ListQueues",
      "sqs:SendMessage",
      "kms:Decrypt",
      "kms:GenerateDataKey"
    ],
    "Resource": ["*"]
  }]
}

